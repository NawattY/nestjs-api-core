{
  "name": "smart-restaurant-api",
  "version": "0.0.1",
  "description": "",
  "author": "dev@ecomlab.dev",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/e2e/jest-e2e.json --maxWorkers=1",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run build && npm run typeorm migration:create -- -o -n",
    "migration:generate": "npm run build && npm run typeorm migration:generate -- -o -n",
    "migration:run": "npm run build && npm run typeorm migration:run",
    "migration:revert": "npm run build && npm run typeorm migration:revert"
  },
  "dependencies": {
    "@appotter/nestjs-s3": "^1.0.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/typeorm": "8.0.4",
    "@nestlab/google-recaptcha": "^3.1.7",
    "@types/hashids": "^2.0.1",
    "aws-sdk": "^2.1196.0",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.5",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.2",
    "graylog2": "^0.2.1",
    "handlebars": "^4.7.7",
    "hashids": "^2.2.10",
    "image-size": "^1.0.2",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.14",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.7.0",
    "nestjs-form-data": "1.7.1",
    "nestjs-graylog": "^1.0.7",
    "nestjs-i18n": "^9.1.8",
    "nestjs-typeorm-paginate": "^3.2.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.4",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.1",
    "sqlite3": "^5.0.11",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.8.1",
    "winston-cloudwatch": "^6.1.1"
  },
  "devDependencies": {
    "@linnify/typeorm-factory": "^1.0.12",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.3",
    "@types/node-fetch": "^2.6.2",
    "@types/passport": "^1.0.9",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/qrcode": "^1.5.0",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.0",
    "jest": "28.1.3",
    "jest-jasmine2": "^28.1.3",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "."
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/app/http/**/*.controller.(t|j)s",
      "src/app/services/**/*.service.(t|j)s"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@enums/(.*)$": "<rootDir>/src/app/common/enums/$1",
      "^@entities/(.*)$": "<rootDir>/src/app/entities/$1",
      "^@services/(.*)$": "<rootDir>/src/app/services/$1",
      "^@middlewares/(.*)$": "<rootDir>/src/app/middlewares/backend/$1",
      "^@providers/(.*)$": "<rootDir>/src/app/providers/$1",
      "^@decorators/(.*)$": "<rootDir>/src/app/common/decorators/$1",
      "^@controller/v1/backend/(.*)$": "<rootDir>/src/app/http/v1/backend/controllers/$1",
      "^@resources/backend/(.*)$": "<rootDir>/src/app/http/v1/backend/resources/$1",
      "^@exceptions/(.*)$": "<rootDir>/src/app/exceptions/$1",
      "^@constants/(.*)$": "<rootDir>/src/app/constants/$1",
      "^@interfaces/(.*)$": "<rootDir>/src/app/common/interfaces/$1",
      "^@helpers/(.*)$": "<rootDir>/src/app/common/helpers/$1",
      "^@dtos/v1/backend/(.*)$": "<rootDir>/src/app/http/v1/backend/dtos/$1",
      "^@resources/frontend/(.*)$": "<rootDir>/src/app/http/v1/frontend/resources/$1",
      "^@controller/v1/frontend/(.*)$": "<rootDir>/src/app/http/v1/frontend/controllers/$1",
      "^@dtos/v1/frontend/(.*)$": "<rootDir>/src/app/http/v1/frontend/dtos/$1"
    },
    "testRunner": "jest-jasmine2"
  }
}
